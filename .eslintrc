{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "brace-style": [2, "stroustrup"],
    "comma-dangle": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "max-len": 0,
    "new-cap": 0,
    "object-curly-spacing": 0,
    "import/prefer-default-export": 0,
    "no-prototype-builtins": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-space-before-closing": 0
  },
  "globals": {
    "afterEach": true,
    "before": true,
    "define": true,
    "describe": true,
    "it": true
  }
}
